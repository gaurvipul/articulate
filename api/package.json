{
  "name": "articulate",
  "version": "0.0.0",
  "description": "Articulate API",
  "author": "Samtec Smart Platform Group",
  "license": "Apache-2.0",
  "main": "server/index.js",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "start": "nodemon server --exec babel-node",
    "start:debug": "nodemon server --exec 'babel-node $NODE_DEBUG_OPTION'",
    "test": "DEBUG=-articulate:* lab -v -a code -T node_modules/lab-transform-preset-env -S $NODE_DEBUG_OPTION",
    "coverage:html": "DEBUG=-articulate:* lab -a code -T node_modules/lab-transform-preset-env -S -c -r html -o coverage.html",
    "lint": "lab -dL -T node_modules/lab-transform-preset-env -S",
    "lint:fix": "lab -dL --lint-fix -T node_modules/lab-transform-preset-env -S"
  },
  "dependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/node": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "axios": "^0.18.0",
    "boom": "^7.2.0",
    "confidence": "3.x.x",
    "debug": "^4.0.1",
    "dotenv": "^6.0.0",
    "elasticsearch": "^15.2.0",
    "glue": "5.x.x",
    "guid": "^0.0.12",
    "handlebars": "^4.0.12",
    "handlebars-helpers": "^0.10.0",
    "handlebars-intl": "^1.1.2",
    "hapi": "17.x.x",
    "hapi-swagger": "^9.3.1",
    "hapi-swaggered": "^3.0.3",
    "hapi-swaggered-ui": "^3.0.2",
    "haute-couture": "3.x.x",
    "inert": "^5.1.0",
    "joi": "13.x.x",
    "json2xml": "^0.1.3",
    "json2yaml": "^1.1.0",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "moment-timezone": "^0.5.23",
    "nes": "^9.0.1",
    "nohm": "^2.2.0",
    "redis": "^2.8.0",
    "require-dir": "^1.0.0",
    "schmervice": "^1.0.0",
    "toys": "2.x.x",
    "vision": "^5.4.0"
  },
  "devDependencies": {
    "code": "5.x.x",
    "eslint": "4.x.x",
    "eslint-config-hapi": "11.x.x",
    "eslint-plugin-hapi": "4.x.x",
    "hpal-debug": "1.x.x",
    "lab": "15.x.x",
    "lab-transform-preset-env": "^1.2.0",
    "nodemon": "^1.18.4"
  },
  "nodemonConfig": {
    "verbose": true,
    "ignore": [
      "node_modules"
    ],
    "delay": "2500"
  }
}
